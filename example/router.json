{
    "/": {
        "action": "index",
        "method": "execute",
        "result": {
            "success": "index.jade"
        }
    },
    "/product": {
        "subs": {
            "/intro": {
                "action": "product/intro",
                "result": {
                    "success": "product/intro.jade"
                }
            },
            "/:productId": {
                "action": "product/detail",
                "result": {
                    "success": "product/detail.jade"
                }
            }
        }
    },
    "/user/:userId": {
        "action": "users",
        "result": {
            "success": "user.jade"
        }
    }

}